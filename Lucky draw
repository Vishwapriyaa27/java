package java1;

import java.util.Scanner;

public class Lucky {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		if(n%10==3|n%10==8) {
			System.out.println("LUCKY WINNER");
		}
		else
		{
			System.out.println("better luck next time");
		}

	}

}
